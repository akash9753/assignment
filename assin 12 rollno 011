Q1)
SELECT CNAME FROM CUSTOMERS
     WHERE SNUM=>
    (SELECT SNUM FROM SALESPEOPLE
     WHERE SNAME='SERRES');
     
     SELECT CNAME,RATING FROM CUSTOMERS WHERE RATING = ANY (SELECT RATING FROM
 CUSTOMERS WHERE SNUM=(SELECT SNUM FROM SALESPEOPLE  WHERE SNAME='SERRES'));
